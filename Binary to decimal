import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
  Scanner sc= new Scanner(System.in);
        int n = sc.nextInt();
        int ans = 0;
        int pv = 1;
        while(n>0){
            ans= ans + (n%10)*pv;
            n=n/10;
            pv = pv * 2;
        }
        System.out.println(ans);
	}
}
