import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
          Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();
        int y = 1;
         while (n> 0){
             y =y*x;
             n = n - 1;
         }
        System.out.println(y);
        
    }
}
