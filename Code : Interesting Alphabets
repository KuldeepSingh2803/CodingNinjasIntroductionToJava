import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
		//Your code goes here
		   Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i = 1;
        while(i<=n){
            int j = 1;
            while (j<=i){
                System.out.print((char)('A'+ n -1 + j - i));
                j++;
            }
            System.out.println();
            i++;
        }
	}
}
